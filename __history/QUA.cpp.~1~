#include "QUA.h"


CQuadratura::CQuadratura(int I, int& A)
{
  this->I = I;
  this->A = A;
  FPDunavant();
}
    
CQuadratura::~CQuadratura()
{
  Xsi.clear();
  W.clear();
}

void Le_Quadraturas(tvQuadratura& Qua, const std::string& NAr)
{
  int nQua;           ///< numero de quadraturas a ser lida;
  char s[1000];       ///< variavel auxiliar
  std::ifstream Ent;  ///< arquivo de leitura
  Ent.open(NAr.c_str());
  Ent.getline(s,1000);
  Ent >>  nQua; Ent.getline(s,1000);
  Ent.getline(s,1000);
  for (int i=0; i<nQua; i++) 
  {
    int I,N;
    Ent >> I >> N; Ent.getline(s,1000);
    CQuadratura LeQua (I,N);
    Qua.push_back(LeQua);
  }
  Ent.close();
}


